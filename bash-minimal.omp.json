{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "type": "rprompt",
            "segments": [
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "#AAECE2",
                    "background": "transparent",
                    "properties": {
                        "time_format": "01/02->15:04"
                    },
                    "template": " {{ .CurrentDate | date .Format }} <#FACE0F>\u276f</>"
                },
                {
                    "type": "executiontime",
                    "style": "plain",
                    "foreground": "#AAECE2",
                    "background": "transparent",
                    "alignment": "left",
                    "properties": {
                        "always_enabled": true,
                        "threshold": 100,
                        "style": "austin"
                    },
                    "template": "{{ .FormattedMs }}"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "root",
                    "style": "plain",
                    "foreground": "#EE0F5A",
                    "background": "transparent",
                    "template": " \u00bb "
                },
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#AAECE2",
                    "background": "transparent",
                    "template": "{{ .UserName }}<#FACE0F>@</>{{ .HostName }} "
                },
                {
                    "type": "os",
                    "style": "plain",
                    "foreground": "#9C77F2",
                    "background": "transparent",
                    "template": "on {{ if .WSL }}WSL at {{ end }}{{ .Icon }} <#FACE0F>\u276f</>"
                },
                {
                    "type": "git",
                    "style": "plain",
                    "foreground": "#AAECE2",
                    "background": "transparent",
                    "properties": {
                        "branch_ahead_icon": "<#EE0F5A>\u2912 </>",
                        "branch_behind_icon": "<#EE0F5A>\u2913 </>",
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "github_icon": "",
                        "source": "cli"
                    },
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }} {{if .BranchStatus }}{{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<#FACE0F> *</>{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }}|{{ end }}{{ if .Staging.Changed }} <#EE0F5A>\uf046</> {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} <#FACE0F>\u276f</>"
                }
            ]
        }
    ],
    "transient_prompt": {
        "background": "transparent",
        "foreground": "#FFFFFF",
        "template": "<#FACE0F>{{ .Shell }}</> \u0023 "
    },
    "final_space": true,
    "version": 2
}
