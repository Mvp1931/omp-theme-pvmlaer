{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "root",
                    "style": "diamond",
                    "foreground": "#010203",
                    "background": "#FACE0F",
                    "template": " \udb85\udc0b ",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b8"
                },
                {
                    "type": "os",
                    "style": "diamond",
                    "foreground": "#077BE7",
                    "background": "#000727",
                    "leading_diamond": "\ue0be",
                    "trailing_diamond": "\ue0b8",
                    "template": " {{ if .WSL }}WSL at {{ end }}{{ .Icon }}  "
                },
                {
                    "type": "path",
                    "style": "diamond",
                    "foreground": "#FACE0F",
                    "background": "#1E1909",
                    "trailing_diamond": "\ue0b0",
                    "properties": {
                        "style": "full"
                    }
                },
                {
                    "type": "git",
                    "style": "diamond",
                    "background": "#0A0B0C",
                    "foreground": "#8543FA",
                    "trailing_diamond": "\ue0b0",
                    "properties": {
                        "branch_ahead_icon": "<#808CEC>\u21e1 </>",
                        "branch_behind_icon": "<#808CEC>\u21e3 </>",
                        "branch_icon": " \ue725 ",
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "github_icon": " \udb80\udea4 ",
                        "source": "cli"
                    },
                    "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }} {{ if .Working.Changed }} <#FFAFD7> *</> {{ .Working.String }}{{ end }} {{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }} {{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} {{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} "
                }
            ]
        }
    ],
    "final_space": true,
    "transient_prompt": {
        "background": "transparent",
        "foreground": "#FFFFFF",
        "template": "<#FACE0F>{{ .UserName }}</> {{ .Shell }} \ue691 "
    },
    "version": 2
}
